{
    "name": "flarum/flarum",
    "description": "Delightfully simple forum software.",
    "type": "project",
    "keywords": ["forum", "discussion"],
    "homepage": "https://flarum.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Toby Zerner",
            "email": "toby.zerner@gmail.com"
        },
        {
            "name": "Franz Liedke",
            "email": "franz@develophp.org"
        }
    ],
    "support": {
        "issues": "https://github.com/flarum/core/issues",
        "source": "https://github.com/flarum/flarum",
        "docs": "https://flarum.org/docs/"
    },
     "repositories": [
        {
            "url": "https://github.com/xiudongy/mixin.git",
            "type": "git"
        }, 
        {
            "url": "https://github.com/xiudongy/flarum-ext-reward-mixin.git",
            "type": "git"
        },
        {
            "url": "https://github.com/xiudongy/flarum-ext-auth-mixin.git",
            "type": "git"
        }
     ],
    "require": {
        "flarum/core": "dev-master",
        "flarum/approval": "dev-master",
        "flarum/auth-facebook": "dev-master",
        "flarum/auth-github": "dev-master",
        "flarum/auth-twitter": "dev-master",
        "flarum/bbcode": "dev-master",
        "flarum/emoji": "dev-master",
        "flarum/lang-english": "dev-master",
        "flarum/flags": "dev-master",
        "flarum/likes": "dev-master",
        "flarum/lock": "dev-master",
        "flarum/markdown": "dev-master",
        "flarum/mentions": "dev-master",
        "flarum/pusher": "dev-master",
        "flarum/statistics": "dev-master",
        "flarum/sticky": "dev-master",
        "flarum/subscriptions": "dev-master",
        "flarum/suspend": "dev-master",
        "flarum/tags": "dev-master",
        "xiudongy/flarum-ext-auth-mixin": "@dev",
        "xiudongy/flarum-ext-reward-mixin": "@dev"
    },
    "require-dev": {
        "zamseam/mixin": "dev-master"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
